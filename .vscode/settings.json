{
  "eslint.format.enable": true,
  "eslint.codeActionsOnSave.rules": null,
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.words": [
    "Attributify",
    "fontshare",
    "Satoshi",
    "Telma",
    "unocss"
  ],
  "files.defaultLanguage": ""
}